{
  "name": "svetch.ts",
  "version": "2.0.3",
  "description": "End-to-End typesafe fetch client for your API",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "svetch": "./dist/bin.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts $PWD",
    "generate": "tsx bin.ts",
    "init": "tsx init.ts"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "typescript",
    "fetch",
    "typesafe"
  ],
  "author": "Omar Al Matar",
  "license": "Paid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bewinxed/svetch"
  },
  "bugs": {
    "url": "https://github.com/Bewinxed/svetch/issues"
  },
  "homepage": "https://github.com/Bewinxed/svetch#readme",
  "dependencies": {
    "comment-parser": "^1.4.1",
    "inquirer": "^9.3.6",
    "json-schema-to-zod": "^1.2.0",
    "readline-sync": "^1.4.10",
    "ts-morph": "^19.0.0",
    "tsx": "^3.14.0",
    "typescript": "^5.5.3",
    "typescript-json-schema": "^0.59.0",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist/esm",
    "moduleResolution": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noEmitOnError": true
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@prisma/client": "5.17.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/detective": "^5.1.5",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.14.11",
    "@types/readline-sync": "^1.4.8",
    "@types/uuid": "^9.0.8",
    "detective-es6": "^4.0.1",
    "heapdump": "^0.3.15",
    "prisma": "^5.17.0",
    "svetch.ts": "^2.0.1",
    "unbuild": "^2.0.0"
  }
}
